<%=render :partial => 'videos/video_player', :locals => { :video => video }%>
<br />
<div class="center">
  <h3>
    <a href=<%="/events/#{video.event.id}"%>>
      <%=video.event.display_name%>
    </a>
  </h3>
  <h2><%= link_to video.title, video_path(video) %></h2>
  <span class="caption"><%= display_presenters_with_links video %></span>
</div>
<div>
  <%=display_video_attributes video%>
</div>
